<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/css/style.css' />
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.js"></script>
    <script src="js/micro-x-tag.js"></script>
    <script>
      (function () {
          var importList = [
              "components/magicbox/magicbox.html",
              "components/hello/hello.html"
          ];
          microXTag.loadImports(importList, function () {
              microXTag.getComponent("magicbox").appendTo(document.body);
          });
      })();
    </script>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <hello></hello>
  </body>
</html>
